refer? ->

function(form) {
console.log(form.getById(10).getValue());

if (form.getById(10).getValue()==='হ্যাঁ') {
       form.getById(11).setHidden(false); 
       form.getById(13).setHidden(false);  
   }else{
       form.getById(11).setHidden(true); 
       form.getById(13).setHidden(true); 
   }
}

end: refer
