function(form) { 
 console.log(form);
 form.getById(6).setHidden(true);

 form.getById(12).setHidden(true);
 
 form.getById(11).setHidden(true);
 form.getById(13).setHidden(true);

}

Health care given? ->

function(form) {
console.log(form.getById(1).getValue());
if (form.getById(1).getValue()==='হ্যাঁ') {
	 form.getById(6).setHidden(false); 
   }else{
	 form.getById(6).setHidden(true);
   }
}

end: health care given?





